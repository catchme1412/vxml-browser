<?xml version="1.0" encoding="UTF-8"?>

<vxml version="2.1">
	<form id="F1">
		<var name="param" />
		<var name="global" />
		<var name="response" expr="param"/>
		<field name="field_confirm" type="boolean">
			<prompt>
				In sub dialog. The value passed is
				<value expr="param" />
			</prompt>
			<return namelist="response"/>
		</field>
	</form>
</vxml>